<autoCompleteControls:EntryBase x:Class="TogglDesktop.AutoCompleteControls.TimerEntry"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:autoCompleteControls="clr-namespace:TogglDesktop.AutoCompleteControls"
             xmlns:toggl="clr-namespace:TogglDesktop"
             mc:Ignorable="d"
             d:DesignWidth="300"
             Height="30"
             Style="{StaticResource AutoCompleteItemBase}">

    <StackPanel Margin="10, 0, 0, 0" Orientation="Horizontal">

        <TextBlock Name="description" x:FieldModifier="private">Description</TextBlock>
        <TextBlock Name="task" x:FieldModifier="private"
                               Foreground="Gray">Task</TextBlock>
        <TextBlock Name="project" x:FieldModifier="private">Project</TextBlock>
        <TextBlock Name="client" x:FieldModifier="private"
                               Foreground="DimGray">Client</TextBlock>

    </StackPanel>
</autoCompleteControls:EntryBase>
